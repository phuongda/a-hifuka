/* mainvisual */
.c-mainvisual
    @include box-100()
    height: calc(100vh - 120px)
    position: relative
    overflow: hidden

    &__cnt
        width: auto
        font-family: 'A-OTF-R'
        position: absolute
        left: calc(100px + 60px)
        bottom: 5.4%
        z-index: 3
        // transform: translateY(-50%)

    h2.c-mainvisual__title
        width: 100%
        margin: 0 0 0 0
        word-break: break-word
        font-weight: 500
        line-height: 1.8
        letter-spacing: 8.6px
        color: white

        span
            display: inline-block
            position: relative

        b
            color: $color-blue

        &:not(:first-child)
            margin-top: -10px

    img.c-mainvisual__title
        width: calc(100% - 120px)
        max-width: 700px
        margin: 0 0 76px 21px

    &__subtitle
        font-weight: 500
        line-height: 1.4

    span.c-mainvisual__subtitle
        display: block
        padding: 10px 10px 8px 10px
        font-size: 2rem
        font-weight: bold
        color: $color-blue
        position: relative

        &::before
            content: ""
            width: 100%
            height: 100%
            background: white
            position: absolute
            top: 2px
            left: 0
            z-index: -1

    h3.c-mainvisual__subtitle
        margin-top: 10px
        // margin-bottom: 36px
        color: white

    &__description
        display: inline-block
        padding: 28px 21px 0 24px
        border-top: 1px solid
        font-size: 1.5rem
        line-height: 1.8
        color: white

    .c-showmore2
        font-size: 2.2rem
        font-weight: 500
        color: white

        .c-icon
            margin-left: 18px

            &.ic-arrow-right6
                width: 52px

    &__bg
        width: calc(100% - 100px)
        height: 100%
        position: absolute
        top: 0
        left: 100px
        z-index: 0

        img,
        video
            @include img-cover()

        &::before
            content: ""
            width: 200px
            height: 100%
            background: #8EAC65
            position: absolute
            top: 0
            left: 0
            z-index: 3

        // &::after
        //     content: ""
        //     width: 100%
        //     height: 100%
        //     // background: rgba(255, 255, 255, 6%)
        //     background: rgba(17, 17, 17, 17%)
        //     position: absolute
        //     top: 0
        //     left: 0
        //     z-index: 2

    &__more
        @include box-100()
        margin-bottom: 45px

        .item
            @include box()
            align-items: center
            margin-bottom: 20px
            font-size: 2rem
            font-weight: 500

            &:not(:last-child)
                margin-right: 20px

            .c-icon
                width: 13px
                height: 13px
                display: block
                margin-right: 10px

    .swiper
        width: 100%
        max-width: 1620px
        padding-right: 100px
        position: absolute
        top: 0
        right: 0
        bottom: 0
        overflow: unset

        &-container
            @include box-100()
            height: 100%
            padding: 0
            overflow: hidden

            &::after
                content: ""
                width: 100%
                height: 100%
                // background: rgba(255, 255, 255, 6%)
                background: rgba(17, 17, 17, 17%)
                position: absolute
                top: 0
                left: 0
                z-index: 2

        .image-container
            height: 100%
            background-position: center
            background-repeat: no-repeat
            background-size: cover
            transition: 10s ease-out
            transform: scale(1)

        .swiper-slide

            &.swiper-slide-active

                .image-container
                    transform: scale(1.2)

        .swiper-pagination
            width: 100px
            height: 100%
            display: flex
            align-items: center
            justify-content: center
            flex-direction: column
            background: white
            top: 0
            right: 0
            left: unset
            z-index: 3

            span
                width: 9px
                height: 9px

                &:not(:last-child)
                    margin-bottom: 11px

            &-bullet

                span
                    background: $color-gray3

                &-active
                    background: $color-green

    .triangle
        width: 0
        height: 0
        border-style: solid
        border-width: 0 900px 760px 435px
        border-color: transparent transparent #191970 transparent
        position: absolute
        left: -325px
        bottom: 0
        z-index: 0

    .l-triangle
        width: 69%
        height: 71%
        max-height: 760px
        position: absolute
        left: -16.95%
        bottom: 0
        z-index: 0

        img
            @include img-contain()
            object-position: bottom

#Y_sclorebox
    position: absolute
    // right: 56px
    right: calc((120px / 2) - 18px)
    bottom: 0
    z-index: 10

#Y_sclore
    max-width: 24px
    display: flex
    flex-direction: row
    align-items: flex-start
    // flex-direction: column
    // align-items: center
    margin: 0 auto

    div
        width: 1px
        height: 150px
        display: block
        background: $color-gray3

    span
        // width: 2px
        width: 1px
        height: 20px
        display: block
        box-sizing: border-box
        -webkit-box-sizing: border-box
        margin: 0 auto
        background: $color-green
        text-align: center
        -webkit-animation: sdb 1.5s infinite
        animation: sdb 1.5s infinite

    p
        width: auto
        margin: 0 0 0 5px
        // margin: 6px 0 0 0
        font-size: 0.9rem
        font-weight: 500
        line-height: 1.4
        letter-spacing: 2px
        font-family: 'A-OTF-R'
        text-align: center
        text-transform: uppercase
        // text-transform: capitalize
        color: $color-gray3
        writing-mode: vertical-rl
        z-index: 99
        transform: rotate(0)

@keyframes sdb
    0%
        transform: rotate(0deg) translate(0, 0)
        opacity: 1
    50%
        opacity: 1
    100%
        transform: rotate(0deg) translate(0, 130px)
        opacity: 1

/* block */
