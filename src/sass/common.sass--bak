/* general style */
@import "core/mixin/_mixin"
@import "core/variable/_color"

/* header */

#main
    @include box-100()

.c-header
    @include box-100()
    height: 100px
    justify-content: space-between
    background: #ffffff
    align-items: center
    transition: .5s

    &.fixed
        position: fixed
        top: 0
        left: 0
        z-index: 100

        &+.clear
            width: 100%
            margin-top: 100px

    &.transparent
        background: transparent

        &+.clear
            display: none

        .c-header__logo
            visibility: hidden

            h1
                color: white

            img
                display: none

        .c-header__slogan
            color: white

        .c-header__menu

            li

                a
                    color: white

                    span
                        color: white

                &:hover
                    color: white

                    &::before
                        opacity: 0.5

                    a
                        opacity: 0.5

                &::before
                    background: white

        .c-header__contact

            a
                color: white

        .btn-toggle
            background: white

            p
                color: $color-blue

            span
                background-color: $color-blue

            &:hover
                background: $color-blue

                p
                    color: white

                span
                    background-color: white

            &.change
                background: $color-blue2

                p
                    color: white

                span
                    background-color: white

    .l-container
        @include box-100()

    &__logo
        font-family: "A-OTF-R"

        &:hover
            opacity: 0.5

        h1
            font-size: 3.2rem
            font-weight: 500
            line-height: 1
            color: $color-green

        span
            font-size: 1.4rem
            font-weight: 500
            color: $color-green

        img
            width: auto
            height: 100%

            &.logo-active
                display: none

        i
            display: block

            &.ic-logo
                width: 272px
                height: 49px

    &__action
        height: 100%
        flex: 1
        display: flex
        justify-content: flex-end
        align-items: center
        padding: 0 0 0 30px

    &__slogan
        display: inline-block
        margin-left: 30px
        font-weight: 700
        line-height: 1.4
        color: $color-blue

    &__contact
        @include box()

        a
            font-size: 1.4rem
            font-weight: 500
            line-height: 1.4
            color: $color-green
            font-family: 'A-OTF-R'
            transition: 0.4s

            &:not(:last-child)
                margin-right: 23px

            span
                //     margin-right: 10px

                &:not(:last-child)
                    margin-bottom: 2px

                b
                    margin-right: 15px

            strong
                font-size: 2.4rem
                font-weight: 500

                small
                    margin-right: 20px
                    font-size: 1.4rem

            &:hover
                opacity: 0.5

    &__action
        height: 100%
        display: flex
        flex: 1
        justify-content: flex-end
        align-items: center
        padding: 0 10px

    &__menu
        width: 100%
        height: 100vh
        display: none
        flex-direction: column
        justify-content: center
        padding: 100px 50px 50px 50px
        background: $color-blue
        font-family: 'A-OTF-R'
        position: absolute
        top: 0
        left: 0
        z-index: -1
        transition: 0.4s

        li
            @include box-100()
            flex-direction: column
            align-items: center
            padding: 12px 20px
            font-size: 2.6rem
            line-height: 1.4

            &:not(:last-child)
                border: 0
                // border-bottom: 1px solid
                // border-color: white!important

            a,
            span
                color: white!important
                transition: 0.4s

                &::after
                    content: unset

                &:hover
                    opacity: 0.5

            ul
                display: none
                margin-bottom: 10px
                padding: 2px 0
                // transition: 0.4s

                li
                    padding: 4px 20px
                    font-size: 2rem

                // &:hover,
                // &:active,
                // &:focus
                //     display: block

            // a:hover,
            // span:hover,
            // a:active,
            // span:active,
            // a:focus,
            // span:focus
            //     opacity: .5

            //     &~ul
            //         display: block

    &.is-show
        color: white

        .c-header

            &__menu
                display: flex

    &.on

        .c-header__action
            padding-right: 0

        .c-header__contact
            width: calc(330px - 100px)
            height: 100%
            align-content: center
            padding-left: 50px
            background: $color-green

            a
                // visibility: hidden
                display: none
                flex-direction: row!important
                margin-right: 0
                font-size: 10px
                color: $color-blue

                &:not(:last-child)
                    margin-bottom: 2px

                &:last-child
                    letter-spacing: 0

                span

                    &:not(:last-child)
                        margin-right: 10px
                        margin-bottom: 0

                b
                    margin-right: 0

        // .btn-toggle
        //     background: white!important

        //     span
        //         background-color: $color-green!important

        //     p
        //         color: $color-green!important

// button toggle
.btn-toggle
    width: 100px
    height: 100%
    @include box()
    flex-flow: row
    flex-direction: column
    justify-content: center
    align-items: center
    padding: 20px
    background: white
    text-align: center
    color: $color-green
    font-family: 'A-OTF-R'
    z-index: 2
    transition: 0.4s
    cursor: pointer

    div
        width: 100%
        max-width: 30px
        @include box()
        flex-direction: column
        align-items: center
        margin: 0 auto

    p
        width: 100%
        margin-bottom: 0
        font-size: 1.1rem
        font-weight: 500
        // letter-spacing: 0
        text-align: center
        color: $color-green
        text-transform: uppercase

    span
        content: ""
        width: 100%
        height: 1px
        display: block
        margin: 5px 0
        background-color: $color-green
        transition: 0.4s

    &.change
        background: $color-green

        span
            background-color: white

            &:nth-of-type(1)
                width: 37px
                transform: rotate(-45deg) translate(-8px, 8px)

            &:nth-of-type(2)
                opacity: 0

            &:nth-of-type(3)
                width: 37px
                transform: rotate(45deg) translate(-8px, -8px)

        p
            color: white

        div
            transform: translateX(-15%)

    &:hover
        background: $color-green
        // opacity: 0.5

        p
            color: white

        span
            background-color: white

.c-menu
    width: 100%
    max-width: 330px
    height: 100vh
    display: flex
    flex-direction: column
    justify-content: flex-center
    align-items: flex-start
    visibility: hidden
    background: $color-green
    color: white
    font-family: 'A-OTF-R'
    position: fixed
    top: 0
    right: 0
    z-index: 99
    transition: 0.5s
    // transform: translateX(100%)
    opacity: 0
    pointer-events: none

    li
        @include box-100()
        flex-direction: column
        align-items: flex-start
        padding: 10px 20px

        &:not(:last-child)
            border: 0
            // border-bottom: 1px solid
            // border-color: white!important

        a
            @include box-100()
            flex-direction: column
            padding-left: 40px
            color: white
            position: relative
            transition: 0.4s

            span
                // margin-top: 2px
                // padding-left: 10px
                font-size: 1.2rem
                line-height: 1.8

            strong
                font-size: 1.8rem
                font-weight: 500
                line-height: 1.4

            &::after
                content: ''
                width: 20px
                height: 1px
                background: white
                position: absolute
                top: 15px
                left: 0

            &:hover
                opacity: 0.5

        ul
            display: none
            margin-bottom: 10px
            padding: 2px 0

            li
                padding: 4px 20px
                font-size: 2rem

    &.is-show
        visibility: visible
        // transform: translateX(0)
        opacity: 1
        pointer-events: unset

    .c-qr
        max-width: 253px
        margin-top: 68px

        div

            &>*
                width: 100%
                max-width: 141px

    .l-contact
        margin-top: 22px
        font-size: 1.4rem
        font-weight: 500

        strong
            margin-left: 20px
            font-size: 2.4rem
            font-weight: 500

.c-action
