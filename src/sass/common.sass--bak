    height: 100%
    @include box()
    flex-direction: column
    justify-content: center
    align-items: center
    background: white
    position: relative
    bottom: 0
    left: 0
    z-index: 99

    &::before
        content: ""
        width: 5px
        height: 100%
        background: linear-gradient(to bottom, #8EAC65, #85AC6A 60%, #65AC7F)
        position: absolute
        top: 0
        left: 0

    #Y_sclorebox
        margin-bottom: 28px
        position: unset
        // transform: translateX(8px)

    &__txt
        // position: absolute
        // top: 12%
        // left: 50%
        // transform: translate(-50%, 0)

        h3
            color: $color-green
            font-weight: 500
            font-family: 'A-OTF-R'
            position: relative

            p
                display: flex
                flex-direction: column

                strong

                    // &:not(last-child)
                    //     margin-bottom: 6px

            strong
                font-size: 1.8rem
                font-weight: 500
                letter-spacing: 4px
                writing-mode: vertical-rl

            span
                margin-left: 10px
                font-size: 1rem
                letter-spacing: 4px
                white-space: nowrap
                writing-mode: vertical-rl
                position: absolute
                top: 50%
                left: 100%
                transform: translateY(-50%)

                small
                    margin-top: 4px
                    margin-left: -4px
                    font-size: 0.9rem

        h4
            margin-top: 17px
            font-size: 1.1rem
            font-weight: 500
            color: $color-gray3
            font-family: 'G-OTF-R'
            writing-mode: vertical-rl
